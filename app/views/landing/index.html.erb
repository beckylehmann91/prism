<body id="landing">
  <div id="app_name" class="flex-container"><h1>PRISM</h1></div>
  <div class="photoShow">
    <div class="current">
      <img src="/seedImages/rainbow_colors.png" >
    </div>
    <div>
      <img src="/seedImages/pixel_rainbow.png" >
    </div>
    <div>
      <img src="/seedImages/grayscale.png" >
    </div>
    <div>
      <img src="/seedImages/dark_sky.png" >
    </div>
    <div>
      <img src="/seedImages/northern_lights.png" >
    </div>
  </div>
  <div class='hexagon'>
    <ul>
      <% if session[:user_id] != nil %>
        <li class='polygon_top'>
          <%= link_to 'Home', root_path %>
        </li>
        <li class='polygon_top'></li>
        <li class='polygon_top'>
          <%= link_to 'Profile', user_path(session[:user_id]) %>
        </li>
        <li class='polygon_bottom'>
          <a href="/posts">Explore</a>
        </li>
        <li class='polygon_bottom'></li>
        <li class='polygon_bottom'>
          <%= link_to 'Logout', logout_path, method: :get %>
        </li>
      <% else %>
        <li class='polygon_top'>
          <%= link_to 'Home', root_path %>
        </li>
        <li class='polygon_top'></li>
        <li class='polygon_top'>
          <%= link_to 'Login', login_path %>
        </li>
        <li class='polygon_bottom'>
          <a href="/posts">Explore</a>
        </li>
        <li class='polygon_bottom'></li>
        <li class='polygon_bottom'>
          <%= link_to 'Signup', new_user_path %>
        </li>
      <% end %>
    </ul>
  </div>
  <div class="spacer"> </div>
</body>
